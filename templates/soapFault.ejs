<env:Envelope xmlns:env="http://www.w3.org/2003/05/soap-envelope" xmlns:fed="http://schemas.xmlsoap.org/ws/2006/12/federation">
 <env:Body>
  <env:Fault>
   <env:Code>
     <env:Value>env:Sender</env:Value>
     <env:Subcode>
      <env:Value><%= code %></env:Value>
     </env:Subcode>
   </env:Code>
   <env:Reason>
     <env:Text xml:lang="en"><%= description %></env:Text>
   </env:Reason>
   <env:Detail>
   </env:Detail>    
  </env:Fault>
 </env:Body>
</env:Envelope>